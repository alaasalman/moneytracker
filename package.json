{
  "name": "moneytracker",
  "description": "A personal finance Vue.js project",
  "version": "1.0.0",
  "author": "Alaa Salman",
  "license": "MIT",
  "private": true,
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/recommended"
    ],
    "plugins": [
      "vue",
      "import"
    ],
    "rules": {
      "semi": [
        "error",
        "always"
      ]
    }
  },
  "scripts": {
    "build": "webpack --progress --mode=production",
    "watch": "webpack --progress --mode=development --watch --watch-poll=1000"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/vue-fontawesome": "^0.1.7",
    "axios": "^0.19.0",
    "bulma": "^0.8.0",
    "chart.js": "^2.9.1",
    "query-string": "^6.8.3",
    "vue": "^2.5.22",
    "vue-bulma-datepicker": "^1.3.6",
    "vue-chartjs": "^3.5.0",
    "vue-multiselect": "^2.1.0",
    "webpack-bundle-tracker": "^0.4.3",
    "chartjs-plugin-colorschemes": "^0.4.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 11"
  ],
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "css-loader": "^3.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-vue": "^5.2.3",
    "file-loader": "^4.2.0",
    "node-sass": "^4.13.0",
    "sass-loader": "^8.0.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "vue-loader": "^15.7.1",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
}
